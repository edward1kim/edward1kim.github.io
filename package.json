{
  "name": "edward1kim.github.io",
  "private": true,
  "description": "Personal website for Edward Kim",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && cp CNAME public && gh-pages -d public",
    "publish": "npm run deploy",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
