    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.11.0",
    "gh-pages": "^3.1.0",
    "prismjs": "^1.23.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "2.2.1"
